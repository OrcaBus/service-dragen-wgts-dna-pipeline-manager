{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$defs": {
    "detailType": {
      "type": "string",
      "enum": ["WorkflowRunStateChange"]
    },
    "status": {
      "type": "string",
      "enum": ["DRAFT"]
    },
    "structure": {
      "type": "string",
      "enum": ["linear", "graph"]
    },
    "timestamp": {
      "pattern": "[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}(?:.[0-9]{3})?Z"
    },
    "workflowName": {
      "type": "string",
      "enum": ["dragen-wgts-dna"]
    },
    "workflowVersion": {
      "pattern": "[0-9]+\\.[0-9]+\\.[0-9]+(?:--*)?"
    },
    "portalRunId": {
      "pattern": "[0-9]{8}[0-9a-f]{8}"
    },
    "libraryObject": {
      "type": "object",
      "properties": {
        "libraryId": {
          "type": "string"
        },
        "orcabusId": {
          "type": "string"
        }
      },
      "required": ["libraryId", "orcabusId"]
    },
    "s3Uri": {
      "pattern": "^s3://[a-zA-Z0-9_-]*/[a-zA-Z0-9_/-]*"
    },
    "s3UriDirectory": {
      "pattern": "^s3://[a-zA-Z0-9_-]*/[a-zA-Z0-9_/-]*/$"
    },
    "reference": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "structure": {
          "$ref": "#/$defs/structure"
        },
        "tarball": {
          "$ref": "#/$defs/s3Uri"
        }
      }
    },
    "fastqListRow": {
      "type": "object",
      "properties": {
        "rgid": {
          "type": "string"
        },
        "rglb": {
          "type": "string"
        },
        "rgsm": {
          "type": "string"
        },
        "lane": {
          "type": "integer"
        },
        "rgcn": {
          "type": "string"
        },
        "rgds": {
          "type": "string"
        },
        "rgdt": {
          "type": "string"
        },
        "rgpl": {
          "type": "string"
        },
        "read1FileUri": {
          "$ref": "#/$defs/s3Uri"
        },
        "read2FileUri": {
          "$ref": "#/$defs/s3Uri"
        }
      },
      "required": ["rgid", "rgsm", "read1FileUri"]
    },
    "sequenceData": {
      "type": "object",
      "properties": {
        "fastqListRows": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/fastqListRow"
          }
        }
      },
      "required": ["fastqListRows"]
    },
    "inputs": {
      "type": "object",
      "properties": {
        "sampleName": {
          "type": "string"
        },
        "tumorSampleName": {
          "type": "string"
        },
        "alignmentOptions": {
          "type": "object",
          "allowAdditionalProperties": true
        },
        "targetedCallerOptions": {
          "type": "object",
          "allowAdditionalProperties": true
        },
        "snvVariantCallerOptions": {
          "type": "object",
          "allowAdditionalProperties": true
        },
        "sequenceData": {
          "$ref": "#/$defs/sequenceData"
        },
        "tumorSequenceData": {
          "$ref": "#/$defs/sequenceData"
        },
        "reference": {
          "$ref": "#/$defs/reference"
        },
        "somaticReference": {
          "$ref": "#/$defs/reference"
        },
        "oraReference": {
          "$ref": "#/$defs/s3Uri"
        }
      },
      "required": [],
      "allowAdditionalProperties": true
    },
    "tags": {
      "type": "object",
      "properties": {
        "libraryId": {
          "type": "string"
        },
        "tumorLibraryId": {
          "type": "string"
        },
        "fastqRgidList": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "tumorFastqRgidList": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "subjectId": {
          "type": "string"
        },
        "individualId": {
          "type": "string"
        }
      },
      "required": ["libraryId"]
    },
    "engineParameters": {
      "type": "object",
      "properties": {
        "projectId": {
          "type": "string"
        },
        "pipelineId": {
          "type": "string"
        },
        "outputUri": {
          "$ref": "#/$defs/s3UriDirectory"
        },
        "logsUri": {
          "$ref": "#/$defs/s3UriDirectory"
        }
      },
      "allowAdditionalProperties": true
    },
    "data": {
      "type": "object",
      "properties": {
        "inputs": {
          "$ref": "#/$defs/inputs"
        },
        "tags": {
          "$ref": "#/$defs/tags"
        },
        "engineParameters": {
          "$ref": "#/$defs/engineParameters"
        }
      },
      "required": ["inputs", "tags"]
    },
    "payload": {
      "type": "object",
      "properties": {
        "version": {
          "type": "string"
        },
        "data": {
          "$ref": "#/$defs/data"
        }
      },
      "required": ["version", "data"]
    },
    "detail": {
      "type": "object",
      "properties": {
        "status": {
          "$ref": "#/$defs/status"
        },
        "timestamp": {
          "$ref": "#/$defs/timestamp"
        },
        "workflowName": {
          "$ref": "#/$defs/workflowName"
        },
        "workflowVersion": {
          "$ref": "#/$defs/workflowVersion"
        },
        "workflowRunName": {
          "type": "string"
        },
        "portalRunId": {
          "$ref": "#/$defs/portalRunId"
        },
        "linkedLibraries": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/libraryObject"
          }
        },
        "payload": {
          "$ref": "#/$defs/payload"
        }
      },
      "required": [
        "status",
        "timestamp",
        "workflowName",
        "workflowVersion",
        "workflowRunName",
        "portalRunId",
        "linkedLibraries",
        "payload"
      ]
    }
  },
  "type": "object",
  "properties": {
    "EventBusName": {
      "type": "string"
    },
    "Source": {
      "type": "string"
    },
    "DetailType": {
      "$ref": "#/$defs/detailType"
    },
    "Detail": {
      "$ref": "#/$defs/detail"
    }
  },
  "required": ["EventBusName", "Source", "DetailType", "Detail"]
}
